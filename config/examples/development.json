{
  "parser": {
    "tree_sitter_enabled": true,
    "clang_enabled": false,
    "target_arch": "x86_64",
    "kernel_version": "6.1",
    "include_paths": [],
    "defines": {},
    "compile_commands_path": null,
    "config_name": "defconfig"
  },
  "call_extraction": {
    "max_file_size": 10485760,
    "enable_parallel": false,
    "min_confidence": "low",
    "include_indirect": true,
    "include_macros": true,
    "include_callbacks": true,
    "include_conditional": true,
    "direct_calls": {
      "include_conditional": true,
      "include_control_flow": true,
      "validate_identifiers": false,
      "max_depth": 50
    },
    "pointer_calls": {
      "include_callback_arrays": true,
      "include_member_pointers": true,
      "validate_identifiers": false,
      "max_depth": 30
    },
    "macro_calls": {
      "include_function_macros": true,
      "include_complex_expansions": true,
      "validate_identifiers": false,
      "max_expansion_depth": 10
    },
    "callbacks": {
      "include_member_calls": true,
      "include_pointer_assignments": true,
      "include_array_callbacks": true,
      "include_callback_registration": true,
      "max_depth": 20
    },
    "conditional": {
      "track_config_dependencies": true,
      "include_ifdef_blocks": true,
      "include_switch_cases": true,
      "max_nesting_depth": 15
    }
  },
  "error_handling": {
    "enable_recovery": true,
    "max_retry_attempts": 3,
    "retry_delay_seconds": 0.1,
    "enable_detailed_logging": true,
    "collect_metrics": true,
    "fail_fast": false
  },
  "logging": {
    "level": "debug",
    "structured": true,
    "file_path": null,
    "enable_metrics": true,
    "format": "pretty"
  },
  "performance": {
    "worker_threads": 1,
    "enable_parallel": false,
    "max_memory_usage": 0,
    "batch_size": 100,
    "cache_size": 1000
  },
  "database": {
    "url": null,
    "pool_size": 5,
    "max_overflow": 10,
    "echo": false,
    "timeout": 30.0
  }
}
